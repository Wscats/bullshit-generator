{
	"name": "bullshift",
	"displayName": "Bullshift Generator",
	"description": "Greatly improve the production efficiency of nonsense.",
	"publisher": "Wscats",
	"icon": "icon.png",
	"author": {
		"name": "Eno Yao",
		"email": "kalone.cool@gmail.com",
		"url": "https://github.com/Wscats"
	},
	"homepage": "https://github.com/Wscats/bullshit-generator#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/Wscats/bullshit-generator"
	},
	"bugs": {
		"url": "https://github.com/Wscats/bullshit-generator/issues",
		"email": "kalone.cool@gmail.com"
	},
	"keywords": [
		"bullshift",
		"generator",
		"nonsense",
		"狗屁不通"
	],
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:bullshift.generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "bullshift.generate",
				"title": "New Bullshift"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"group": "navigation",
					"command": "bullshift.generate"
				}
			]
		},
		"configuration": {
			"title": "Bullshift Setting",
			"properties": {
				"bullshift.language": {
					"type": "string",
					"default": "CN",
					"enum": [
						"EN",
						"CN"
					],
					"description": "Set the generated language."
				}
			}
		}
	},
	"scripts": {
		"build": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}